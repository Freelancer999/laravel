/*
 *  STYLE 4
 */

.scrollbar
{
	margin-left: 30px;
	float: left;
	height: 220px;
	width: 100%;
	background: #FFFFF;
	overflow-y: scroll;
	margin-bottom: 25px;
}

.force-overflow
{
	
}

#wrapper
{
	text-align: center;
	width: 500px;
	margin: auto;
}

#style-4::-webkit-scrollbar-track
{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #F5F5F5;
}

#style-4::-webkit-scrollbar
{
	width: 10px;
	background-color: #F5F5F5;
}

#style-4::-webkit-scrollbar-thumb
{
	background-color: orange;
	border: 2px solid orange;
}

#style-4  {
  margin-left:-2px;
  margin-left::1px;
}

